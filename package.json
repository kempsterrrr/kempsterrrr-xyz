{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "framer-motion": "^1.9.1",
    "gatsby": "^2.15.29",
    "gatsby-image": "^2.2.24",
    "gatsby-plugin-manifest": "^2.2.20",
    "gatsby-plugin-netlify": "^2.1.22",
    "gatsby-plugin-offline": "^3.0.11",
    "gatsby-plugin-page-creator": "^2.1.27",
    "gatsby-plugin-react-helmet": "^3.1.10",
    "gatsby-plugin-react-svg": "^2.1.2",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sharp": "^2.2.28",
    "gatsby-plugin-sitemap": "^2.2.19",
    "gatsby-plugin-typography": "^2.3.12",
    "gatsby-remark-images": "^3.1.28",
    "gatsby-remark-prismjs": "^3.3.20",
    "gatsby-source-filesystem": "^2.1.29",
    "gatsby-source-twitter": "^3.0.0",
    "gatsby-transformer-remark": "^5.25.1",
    "gatsby-transformer-sharp": "^2.2.23",
    "moment": "^2.24.0",
    "prismjs": "^1.17.1",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-github-btn": "^1.1.0",
    "react-helmet": "^5.2.1",
    "react-twitter-embed": "^2.0.8",
    "react-typography": "^0.16.19",
    "typeface-roboto-slab": "^0.0.72",
    "typeface-ruda": "^0.0.72",
    "typography": "^0.16.19"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.1.1",
    "@testing-library/react": "^9.3.0",
    "babel-jest": "^24.9.0",
    "babel-preset-gatsby": "^0.2.17",
    "eslint-config-prettier": "^6.4.0",
    "eslint-config-react-app": "^5.0.2",
    "husky": "^3.0.9",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.10.2"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --ignore-path .gitignore --ignore-path .prettierignore --fix",
      "git add"
    ],
    "*.{md,css,scss,yaml,yml}": [
      "prettier --write",
      "git add"
    ],
    "*.svg": [
      "svgo --pretty --indent=2 --config=svgo.yml --multipass",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "start:clean": "rm -rf .cache && npm run develop",
    "serve": "gatsby serve",
    "test": "jest",
    "lint": "eslint --ignore-path .gitignore --ignore-path .prettierignore --ext .js,.jsx ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
